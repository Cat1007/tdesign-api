{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-param-reassign": 0,
    "camelcase": 1,
    "no-new": 1,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "func-style": 0,
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "@typescript-eslint/no-explicit-any": 0
  }
}
